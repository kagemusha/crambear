<div class="tag-name">
  {{tag.name}}
</div>
{{#card-tag-autocomplete
    options=unusedSetTags
    inputClass="card-tag-input"
    selectedValue=newCardTag
    unusedSetTags=unusedSetTags
    selectItem="tagSelected"
    hideWhenNoSuggestions=true
    as |setTag|
}}
  <p>{{setTag.name}}</p>
{{/card-tag-autocomplete}}
