<div id="set-nav">
  <h3>My Sets</h3>
  {{#if isLoggedIn}}
    (show userSets)
  {{else}}
    (Login to view) - todo
  {{/if}}

  <h3>Public Sets</h3>
  <ul class="card-sets">
    {{#each set in publicSets}}
      {{#link-to "card-set" set tagName="li" class='li-link'}}{{set.name}} ({{set.cardCount}}){{/link-to}}
    {{/each}}
  </ul>
</div>
